<odoo>
    <data>
        <record id="offer_mail_template" model="mail.template">
            <field name="name">Ticket Infomation</field>
            <field name="email_from">test@qa.team</field>
            <field name="email_to">{{object.email}}</field>
            <field name="model_id" ref="real-estate-ads.model_support_ticket"/>
            <field name="auto_delete" eval="True"/>
            <field name="subject">New Infomation on {object.name}</field>
            <field name="body_html" type="html">
                <p>
                    Dear Bidders, 
                    <br/>
                    This ticket is now Sold
                    <br/>
                    Regards,
                    <br/>
                    odoo_1 Ticket
                </p>
            </field>
        </record>
    </data>
</odoo>